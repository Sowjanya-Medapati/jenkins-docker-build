pipeline {
    agent {
     docker {
      image 'maven:3.8.3-openjdk-17'
      }
     }

    stages {
        stage('build') {
            steps {
                echo "Hello world !!"
                echo "Hello Sowjanya !!!!"
                sh 'mvn -B -DskipTests clean package'
                echo "done"
            }
        }
    }
}
